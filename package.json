  "scripts": {
    "bump": "npm version",
    "build": "yarn clean && gatsby build && yarn build-storybook",
    "build-storybook": "build-storybook",
    "clean": "gatsby clean",
    "develop": "yarn clean && concurrently --names 'STORYBOOK' -c 'bgMagenta.bold' 'yarn storybook'",
    "format": "yarn format-js && yarn format-css && echo '> Finished formatting ✅'",
    "format-css": "yarn lint-css --fix",
    "format-js": "prettier-eslint --eslint-config-path ./config/eslint.config.js --trailing-comma es5 --single-quote --write $PWD'/**/*.js'",
    "jest": "jest --config ./config/jest.config.js",
    "lint-css": "stylelint 'src/assets/stylesheets/**/*.scss' --config ./config/stylelint.config.js",
    "lint-js": "eslint -c ./config/eslint.config.js . --ext .js",
    "lint": "yarn lint-js && yarn lint-css && echo '> Finished linting ✅'",
    "serve": "gatsby serve",
    "snyk:wizard": "synk wizard",
    "start": "npm run develop",
    "storybook": "start-storybook -s ./src/stories/static -p 6006",
    "test-clean": "yarn jest -u",
    "test-watch": "yarn jest --watchAll",
    "test": "yarn jest && echo '> Finished testing ✅'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "yarn format-js",
      "yarn lint-js",
      "git add"
    ],
    "**/*.scss": [
      "yarn format-css",
      "yarn lint-css",
      "git add"
    ]
  },
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "https://github.com/Element84/fogg"
  },
  "bugs": {
    "url": "https://github.com/Element84/fogg/issues"
  }

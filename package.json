    "react-helmet": "^5.2.0",
    "react-icons": "^3.6.1",
    "react-leaflet": "^2.2.1",
    "react-leaflet-draw": "^0.19.0",
    "sass": "^1.17.2",
    "use-debounce": "^2.0.0",
    "uuid": "^3.3.2",
    "yarn": "^1.15.2"
  },
  "devDependencies": {
    "@fabiospampinato/bump": "^1.0.6",
    "@storybook/addon-actions": "^5.0.3",
    "@storybook/addon-links": "^5.0.1",
    "@storybook/addons": "^5.0.1",
    "@storybook/cli": "^5.0.1",
    "@storybook/react": "^5.0.1",
    "babel-jest": "^24.5.0",
    "concurrently": "^4.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "^5.15.3",
    "eslint-config-semistandard": "^13.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "esri-leaflet": "^2.2.4",
    "esri-leaflet-geocoder": "^2.2.14",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.7.1",
    "leaflet": "^1.4.0",
    "lint-staged": "^8.1.5",
    "moxios": "^0.4.0",
    "prettier": "^1.16.4",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "semistandard": "^13.0.1",
    "sinon": "^7.2.7",
    "stylelint": "^9.10.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "bump": "bump",
    "build": "gatsby build && yarn build-storybook",
    "build-storybook": "build-storybook",
    "develop": "rm -rf .cache && concurrently --names 'GATSBY,STORYBOOK' -c 'bgCyan.bold,bgMagenta.bold' 'gatsby develop' 'yarn storybook'",
    "format": "yarn format-js && yarn lint-css --fix && echo '> Finished formatting ✅'",
    "format-js": "prettier-eslint --eslint-config-path ./config/eslint.config.js --trailing-comma es5 --single-quote --write '**/*.js'",
    "lint-css": "stylelint 'src/assets/stylesheets/**/*.scss' --config ./config/stylelint.config.js",
    "lint-js": "eslint -c ./config/eslint.config.js . --ext .js",
    "lint": "yarn lint-js && yarn lint-css && echo '> Finished linting ✅'",
    "precommit": "lint-staged",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "storybook": "start-storybook -p 6006",
    "test-watch": "jest --watchAll --config ./config/jest.config.js",
    "test": "jest --config ./config/jest.config.js && echo '> Finished testing ✅'"
  },
  "lint-staged": {
    "**/*.js": [
      "yarn format",
      "yarn lint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://repo.element84.com/SpaceJam/fogg"
  },
  "main": "n/a"
}

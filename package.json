    "gatsby": "^2.18.4",
    "leaflet": "1.6.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@fabiospampinato/bump": "^1.2.0",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/cli": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "babel-jest": "^24.8.0",
    "concurrently": "^5.0.2",
    "core-js": "2.6.5",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "6.0.1",
    "eslint-config-semistandard": "^15.0.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-standard": "^4.0.1",
    "gatsby": "^2.18.16",
    "husky": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "leaflet": "1.6.0",
    "lint-staged": "^9.5.0",
    "moxios": "^0.4.0",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1",
    "prettier-eslint-cli": "^5.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "semistandard": "^14.2.0",
    "sinon": "^7.5.0",
    "stylelint": "^12.0.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "bump": "bump",
    "build": "yarn clean && gatsby build && yarn build-storybook",
    "build-storybook": "build-storybook",
    "clean": "gatsby clean",
    "develop": "yarn clean && concurrently --names 'STORYBOOK' -c 'bgMagenta.bold' 'yarn storybook'",
    "format": "yarn format-js && yarn format-css && echo '> Finished formatting ✅'",
    "format-css": "yarn lint-css --fix",
    "format-js": "prettier-eslint --eslint-config-path ./config/eslint.config.js --trailing-comma es5 --single-quote --write '**/*.js'",
    "lint-css": "stylelint 'src/assets/stylesheets/**/*.scss' --config ./config/stylelint.config.js",
    "lint-js": "eslint -c ./config/eslint.config.js . --ext .js",
    "lint": "yarn lint-js && yarn lint-css && echo '> Finished linting ✅'",
    "precommit": "lint-staged",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "storybook": "start-storybook -s ./src/stories/static -p 6006",
    "test-watch": "jest --watchAll --config ./config/jest.config.js",
    "test": "jest --config ./config/jest.config.js && echo '> Finished testing ✅'"
  },
  "lint-staged": {
    "**/*.js": [
      "yarn format-js",
      "yarn lint-js",
      "git add"
    ],
    "**/*.scss": [
      "yarn format-css",
      "yarn lint-css",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://repo.element84.com/fogg/fogg"
  },
  "main": "n/a"
}

    "gatsby-plugin-manifest": "^2.0.20",
    "gatsby-plugin-offline": "^2.0.24",
    "gatsby-plugin-react-helmet": "^3.0.7",
    "gatsby-plugin-sass": "^2.0.10",
    "gatsby-plugin-sharp": "^2.0.23",
    "gatsby-source-filesystem": "^2.0.23",
    "gatsby-transformer-sharp": "^2.1.14",
    "geojson": "^0.5.0",
    "leaflet-draw": "^1.0.4",
    "mkdirp": "^0.5.1",
    "moment": "^2.24.0",
    "node-sass": "^4.11.0",
    "normalize.css": "^8.0.1",
    "p-cancelable": "^2.0.0",
    "path": "^0.12.7",
    "proj4": "^2.5.0",
    "proj4leaflet": "^1.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-datetime": "^2.16.3",

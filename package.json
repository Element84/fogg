    "leaflet-draw": "^1.0.4",
    "mkdirp": "^0.5.1",
    "moment": "^2.24.0",
    "node-sass": "^4.13.0",
    "normalize.css": "^8.0.1",
    "p-cancelable": "^2.0.0",
    "path": "^0.12.7",
    "proj4": "^2.6.0",
    "proj4leaflet": "^1.0.2",
    "prop-types": "^15.7.2",
    "react-datetime": "^2.16.3",
    "react-helmet": "^5.2.1",
    "react-icons": "^3.8.0",
    "react-input-range": "^1.3.0",
    "react-leaflet": "^2.6.1",
    "react-leaflet-draw": "^0.19.0",

  "dependencies": {
    "fogg": "^0.3.0",
    "gatsby": "2.23.22",
    "gatsby-plugin-manifest": "^2.4.17",
    "gatsby-plugin-react-helmet": "^3.3.9",
    "gatsby-plugin-react-leaflet": "2.0.13",
    "gatsby-plugin-resolve-src": "^2.1.0",
    "gatsby-plugin-sass": "^2.3.10",
    "leaflet": "^1.6.0",
    "node-sass": "^4.14.1",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^3.10.0",
    "react-leaflet": "^2.7.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "babel-preset-gatsby": "^0.5.1",
    "enzyme": "^3.11.0",
    "eslint": "^7.4.0",
    "eslint-config-zurgbot": "^0.0.7",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.5",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "prettier-eslint": "^11.0.0",
    "prettier-eslint-cli": "^5.0.0"
  },
  "keywords": [
    "gatsby",
    "sass",
    "leaflet",
    "mapping",
    "maps"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "bump": "npm version",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "format": "prettier-eslint --eslint-config-path ./config/eslint.config.js --write $PWD/'**/*.js'",
    "lint": "eslint -c ./config/eslint.config.js . --ext .js",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "test": "jest --config ./config/jest.config.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "yarn format",
      "yarn lint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Element84/fogg"
  },
  "bugs": {
